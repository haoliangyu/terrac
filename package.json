{
  "name": "tshare",
  "version": "0.0.0",
  "description": "oclif example Hello World CLI",
  "author": "Haoliang Yu @haoliangyu",
  "bin": {
    "tshare": "./bin/run"
  },
  "homepage": "https://github.com/haoliangyu/tshare",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "haoliangyu/tshare",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.259.0",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.2.2",
    "fs-extra": "^11.1.0",
    "simple-git": "^3.16.0",
    "zip-a-folder": "^1.1.5"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.3",
    "@types/chai": "^4",
    "@types/fs-extra": "^11.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.11",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "husky": "^8.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "oclif": {
    "bin": "tshare",
    "dirname": "tshare",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lint:fix": "eslint . --ext .ts --config .eslintrc --fix",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/haoliangyu/tshare/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
